{
  "name": "cli",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "type": "module",
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup src/index.ts --format cjs",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "start": "node dist/index.js",
    "lint": "eslint src --cache --cache-strategy content",
    "format": "prettier --write --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "format:check": "prettier --check --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "release": "changeset version",
    "pub:beta": "pnpm build && npm publish --tag beta",
    "pub:next": "pnpm build && npm publish --tag next",
    "pub:release": "pnpm build && npm publish"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@snippy/api": "*",
    "@trpc/client": "10.0.0-proxy-beta.15",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "inquirer": "^9.1.3",
    "kleur": "^4.1.5",
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.2",
    "@types/node": "18.7.23",
    "eslint": "8.22.0",
    "pkg": "^5.8.0",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  }
}
