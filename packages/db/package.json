{
  "name": "@snippy/db",
  "version": "0.0.0",
  "main": "./index.ts",
  "type": "module",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup index.ts --format esm --clean --sourcemap --minify --metafile --dts",
    "db:rm": "docker compose rm db -s -f -v",
    "db:up": "docker compose up db -d",
    "db:push": "prisma db push",
    "db:deploy": "prisma db deploy",
    "db:generate": "prisma db generate",
    "db:studio": "prisma studio --port 5556"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1"
  },
  "devDependencies": {
    "prisma": "^4.3.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
